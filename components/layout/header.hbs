<div class="panel">
	<header>
		<div class="logo-wrapper">
			<a href="/" class="logo">
				{{>logo}}
			</a>
		</div>

		<div class="menu">
			<ul>
				<li><a href="/">{{global.mainmenu.homelabel}}</a></li>
				{{#each global.mainmenu.items}}
					<li><a href="/{{link}}">{{label}}</a></li>
				{{/each}}

			</ul>
		</div>

		<div class="clearfix"></div>
	</header>
</div>